{
    "name": "dillingham/nova-redirect-detail",
    "description": "Redirect Laravel Nova detail views to edit",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Dillingham\\NovaRedirectDetail\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dillingham\\NovaRedirectDetail\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
